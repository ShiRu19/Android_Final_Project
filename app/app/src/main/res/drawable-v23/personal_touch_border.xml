<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:left="5dp"
        android:right="5dp"
        android:end="5dp"
        android:start="5dp"
        android:top="5dp"
        android:bottom="5dp">
        <shape
            android:shape="rectangle" >
            <solid android:color="@android:color/white" />
            <stroke
                android:width="1dp"
                android:color="@color/teal_A700" />
            <corners android:radius="5dp" />
        </shape>
    </item>
    <item
        android:height="10dp"
        android:width="10dp"
        android:right="0dp"
        android:top="0dp"
        android:gravity="right">

        <shape android:shape="oval">
            <solid android:color="@color/teal_A700"/>
            <size android:width="5dp"
                android:height="5dp"/>
        </shape>

    </item>
</layer-list>